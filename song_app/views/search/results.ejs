<section>
    <h2>Search Results</h2>
    <div>
        <% if(songs.length > 0) { %>
            <section class="song-list">
                <% songs.forEach(function(song){ %>
                    <article class="song">
                    <dl>
                        <dt>song artist:</dt>
                        <dd><%=song.Artist %>
                        </dd>
                    </dl>
                    <dl>
                        <dt>title:</dt>
                        <dd>
                            <%=song.Title %>
                        </dd>
                        </dl>
                        <dl>
                        <dt>chords available:</dt>
                        <dd>
                            <%=song.Chords %>
                        </dd>
                        
                        </dl>
                        
                    </article>
                
                <% }); %> 
                <p><a href="#" onclick="history.back();"> << Back to Search</a></p>   
            </section>
        <% } else{ %>                
             <section class = "wrapper-center">    
             <p>No matching results, try again</p>
            <p><a href="#" onclick="history.back();"> << Back to Search</a></p>
            </section>
        <% } %>
    </div>
<section>
